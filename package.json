  "scripts": {
    "start": "react-scripts start",
    "start:clean": "set DISABLE_ESLINT_PLUGIN=true && set ESLINT_NO_DEV_ERRORS=true && react-scripts start",
    "start:no-eslint": "set DISABLE_ESLINT_PLUGIN=true && set ESLINT_NO_DEV_ERRORS=true && set GENERATE_SOURCEMAP=false && react-scripts start",
    "start:windows": "powershell -Command \"$env:DISABLE_ESLINT_PLUGIN='true'; $env:ESLINT_NO_DEV_ERRORS='true'; npm start\"",
    "prebuild": "node scripts/generateVersionInfo.js",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "seed:concepts": "ts-node scripts/seedConcepts.ts",
    "seed:categories": "ts-node scripts/seedCategories.ts",
    "convert:local-to-mongo": "ts-node scripts/convertLocalToMongo.ts",
    "seed:from-local": "ts-node scripts/seedFromLocal.ts",
    "seed:from-mongo-ready": "node scripts/seedFromMongoReady.js",
    "update:production": "node scripts/updateProductionData.js",
    "version:info": "node scripts/generateVersionInfo.js",
    "test:pdf": "node test-pdf-extraction.js",
    "test:server": "node test-server-endpoints.js",
    "debug:masterlist": "node debug-master-list.js",
    "test:save-article": "node test-save-article.js",
    "update:latest": "node scripts/updateToLatestBackup.js",
    "seed:canonical": "node scripts/seedFromCanonicalJson.js",
    "typecheck": "tsc --noEmit",
    "validate:data": "node scripts/validateCanonicalJson.js",
    "clear-cache": "powershell -Command \"Remove-Item -Path 'node_modules\\.cache' -Recurse -Force -ErrorAction SilentlyContinue\"",
    "dev": "npm run clear-cache && npm run start:clean"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/d3": "^7.4.3",
    "@types/matter-js": "^0.19.8",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
